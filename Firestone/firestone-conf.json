// Firestone configuration

{
  // Lobby server IP address binding
  "lobby_server_ip": "127.0.0.1",

  // Lobby server port binding
  "lobby_server_port": 1119,

  // Path to server SSL PFX file (public certificate and private key)
  "lobby_server_certificate_file": "Firestone.pfx",

  // Client timeout (seconds)
  "client_connection_timeout": 10
}
